	"license": "GPL-3.0",
	"devDependencies": {
		"@types/jest": "^26.0.23",
		"jest": "^27.0.4",
		"prettier": "^2.3.1",
		"ts-jest": "^27.0.3",
		"ts-node": "^10.0.0",
		"typescript": "^4.3.2",
		"tslint": "^6.1.3",
		"tslint-config-prettier": "^1.18.0"
	}
}
